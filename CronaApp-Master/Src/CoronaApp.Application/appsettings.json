{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },

    "ConnectionStrings": {
        "myconn": "server=.; database=Covid;Trusted_Connection=True;"
    },
    "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "myconn",
          "tableName": "Log",
          "autoCreateSqlTable": true
        }
      }
    ]
  }
}
